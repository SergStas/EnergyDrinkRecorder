<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sergstas.energydrinkrecorder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.EntriesActivity" android:label="@string/entriesActivity_title"/>
        <activity android:name=".activities.PositionsActivity" android:label="@string/activity_positions_title"/>
        <activity android:name=".activities.NewEntryActivity" android:label="@string/activity_newEntry_title"/>
        <activity android:name=".activities.NewPositionActivity" android:label="@string/activity_newPosition_title" />
    </application>

</manifest>